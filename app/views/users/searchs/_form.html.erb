<%= form_with url:search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content, class: 'form_control', value: params[:content] %>
  <%= f.select :model,
                     options_for_select({'ユーザー'=>'user','レシピ'=>'recipe','アイテム'=>'equipment','ノート'=>'note'}, params[:model]),
                     class: 'form-control form-control-sm'
  %>
  <%= f.submit '検索', class: 'btn btn-primary', name: nil %>
<% end %>